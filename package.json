{
  "name": "hoarding-backend",
  "version": "0.1.0",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon --watch src --exec npx ts-node src/index.ts",
    "build": "tsc -p .",
    "start": "node dist/index.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:studio": "prisma studio",
    "seed": "npx ts-node prisma/seed.ts",
    "import:hoardings": "npx ts-node scripts/import-hoardings.ts",
    "regenerate:codes": "npx ts-node scripts/regenerate-hoarding-codes.ts",
    "check:landlords": "npx ts-node scripts/check-missing-landlords.ts",
    "lint": "eslint src/**/*.{ts,js} prisma/**/*.{ts,js} scripts/**/*.{ts,js} --fix",
    "test": "cross-env NODE_ENV=test jest --runInBand",
    "test:unit": "cross-env NODE_ENV=test jest --runInBand --selectProjects unit",
    "test:integration": "cross-env NODE_ENV=test jest --runInBand --selectProjects integration",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage --runInBand"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.25",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/nodemailer": "^7.0.4",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.18.2",
    "fs": "^0.0.1-security",
    "helmet": "^7.0.0",
    "ioredis": "^5.0.0",
    "jsonwebtoken": "^9.0.0",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.10",
    "path": "^0.12.7",
    "process": "^0.11.10",
    "uuid": "^9.0.1",
    "winston": "^3.0.0",
    "zod": "^3.0.0"
  },
  "optionalDependencies": {
    "get-stream": "^6.0.1",
    "pdfkit": "^0.13.0"
  },
  "devDependencies": {
    "@types/helmet": "^0.0.48",
    "@types/jest": "^29.5.14",
    "@types/multer": "^1.4.13",
    "@types/node": "^25.0.2",
    "@types/pdfkit": "^0.12.0",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "autoprefixer": "^10.4.23",
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "cross-env": "^10.1.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^29.7.0",
    "nodemon": "^2.0.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prisma": "^5.22.0",
    "supertest": "^6.3.4",
    "tailwindcss": "^3.4.19",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
